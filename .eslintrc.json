{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console":"off",
        "no-underscore-dangle":"off",
        "object-curly-newline":"off",
        "operator-linebreak" :"off"
    }
}
